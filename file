PROGRAM TEST
    INTEGER VARIABLE_1, VARIABLE_2
    DIMENSION ARRAY(10)
    CHARACTER LINE
    REAL VARIABLE_3, VARIABLE_4
    VARIABLE_1 = 0
    VARIABLE_2 = 10
    DO WHILE (VARIABLE_2 .GE. VARIABLE_1)
        ARRAY[VARIABLE_1] = VARIABLE_1 ** 2
        VARIABLE_1 = VARIABLE_1 + 1
    END DO

    LINE = "Some text"
    IF (LINE .NE. "Another text") WRITE (*,*) LINE

    VARIABLE_3 = .2E-6 - 2
    VARIABLE_4 = 0.24 * 12 + (5 * 4 / 10)
    WRITE (*,*) VARIABLE_3 / VARIABLE_4
END PROGRAM TEST