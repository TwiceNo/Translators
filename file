PROGRAM TEST
    INTEGER VARIABLE_1, VARIABLE_2, A, B
    CHARACTER LINE
    REAL VARIABLE_3, VARIABLE_4
    VARIABLE_1 = 0
    VARIABLE_2 = 10
    DO WHILE (VARIABLE_1 .LT. VARIABLE_2)
        VARIABLE_1 = VARIABLE_1 + 1
    END
    LINE = "Some text"
    IF (LINE .NE. "Another text") VARIABLE_2 = 34
    CALL MY_FUNC(A + B, B * A)
    VARIABLE_3 = .2E-6 - 2
    VARIABLE_4 = 0.24 * 12 + (5 * 4 / 10)
END
